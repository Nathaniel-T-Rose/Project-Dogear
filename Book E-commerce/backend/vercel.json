{
    "version": 2,
    "builds": [
      {
        "src": "bookcommerce/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "bookcommerce/wsgi.py"
      }
    ],
    "installCommand": "pip install -r requirements.txt"
  }